{
  "name": "xlayer_pumpfun",
  "version": "1.0.0",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "test:gas": "REPORT_GAS=true npx hardhat test",
    "deploy:testnet": "npx hardhat run scripts/deploy.js --network xlayer",
    "deploy:mainnet": "npx hardhat run scripts/deploy.js --network xlayerMainnet",
    "deploy:dex": "npx hardhat run scripts/deployDEX.js --network xlayer",
    "check:balance": "npx hardhat run scripts/getTestTokens.js --network xlayer",
    "verify:testnet": "npx hardhat verify --network xlayer",
    "verify:mainnet": "npx hardhat verify --network xlayerMainnet",
    "clean": "npx hardhat clean",
    "node": "npx hardhat node",
    "frontend": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run node\" \"npm run frontend\""
  },
